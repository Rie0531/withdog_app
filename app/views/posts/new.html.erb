<div class="contents row">
  <div class="container">
    <h3>投稿する</h3>
    <%= form_with(model: @post, local: true) do |form| %>
      <%= form.text_field :name, placeholder: "施設名" %>
      <%= form.text_field :day, placeholder: "訪問年月"%>
      <%= form.text_field :dogtype, placeholder: "ペットの種類"%>
      <%= form.text_field :image, placeholder: "Image Url" %>
      <%= form.text_area :text, placeholder: "text", rows: "10" %>
      <%= form.submit "POST" %>
    <% end %>
  </div>
</div>